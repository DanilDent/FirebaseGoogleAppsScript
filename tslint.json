{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "rules": {
    "no-namespace": false,
    "no-reference": false,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "//"
      }
    ]
  },
  "jsRules": {},
  "rulesDirectory": []
}
